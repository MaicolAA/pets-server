{
    "version": 2,
    "builds": [
      {
        "src": "api/index.ts",
        "use": "@vercel/node",
        "config": {
          "distDir": "dist",
          "buildCommand": "npm run build:vercel", 
          "installCommand": "npm install",
          "outputDirectory": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.ts"
      }
    ]
  }